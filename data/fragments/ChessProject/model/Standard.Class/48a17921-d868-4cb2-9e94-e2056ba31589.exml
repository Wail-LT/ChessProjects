                <ID name="" mc="Infrastructure.Note" uid="43d5f9a9-cf61-47bf-8ffa-28f0aef3a081"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[if(c1.getPiece().getCouleur()==Couleur.blanc)
{
    if ((c2.getRange()==c1.getRange()+1 && c2.getColonne()==c1.getColonne() && c2.getPiece()==null) || (c2.getRange()==c1.getRange()+1 && (c2.getColonne()==c1.getColonne()+1 || c2.getColonne()==c1.getColonne()-1)&& c2.getPiece()!=null && (c2.getPiece().getCouleur() != Couleur.blanc)))
        return true;
    if (c1.getPiece().isDebut() && c2.getRange()==c1.getRange()+2 && c2.getColonne()==c1.getColonne())
        return true;
}
else
{
    if ((c2.getRange()==c1.getRange()-1 && c2.getColonne()==c1.getColonne() && c2.getPiece()==null) || (c2.getRange()==c1.getRange()-1 && (c2.getColonne()==c1.getColonne()+1 || c2.getColonne()==c1.getColonne()-1)&& c2.getPiece()!=null && (c2.getPiece().getCouleur() != Couleur.noir)))
        return true;
    if (c1.getPiece().isDebut() && c2.getRange()==c1.getRange()-2)
        return true;
}
]]></ATT>
