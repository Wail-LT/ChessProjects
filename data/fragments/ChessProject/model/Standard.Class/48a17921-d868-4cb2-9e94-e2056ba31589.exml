              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="ea00207d-f5b9-4f3f-b3b9-2724a824c35c"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Case tab[] = new Case[3];
int j=0;
if (couleur==Couleur.blanc)
{
    if (Pion.verification(this.getCase(),Echiquier.ech.gcase(cases.getRange()+1,cases.getColonne())))
    {
        tab[j]=Echiquier.ech.gcase(cases.getRange()+1, cases.getColonne());
        j++;
    }
    if (Pion.verification(this.getCase(), Echiquier.ech.gcase(cases.getRange()+2,cases.getColonne())))
    {
        tab[j]=Echiquier.ech.gcase(cases.getRange()+2, cases.getColonne());
        j++;
    }
    if (Pion.verification(this.getCase(), Echiquier.ech.gcase(cases.getRange()+1,cases.getColonne()+1)))
    {
        tab[j]=Echiquier.ech.gcase(cases.getRange()+1, cases.getColonne()+1);
        j++;
    }
    if (Pion.verification(this.getCase(), Echiquier.ech.gcase(cases.getRange()+1,cases.getColonne()-1)))
    {
        tab[j]=Echiquier.ech.gcase(cases.getRange()+1, cases.getColonne()-1);
        j++;
    }
}
else 
{
    if (Pion.verification(this.getCase(),Echiquier.ech.gcase(cases.getRange()-1,cases.getColonne())))
    {
        tab[j]=Echiquier.ech.gcase(cases.getRange()-1, cases.getColonne());
        j++;
    }
    if (Pion.verification(this.getCase(), Echiquier.ech.gcase(cases.getRange()-2,cases.getColonne())))
    {
        tab[j]=Echiquier.ech.gcase(cases.getRange()-2, cases.getColonne());
        j++;
    }
    if (Pion.verification(this.getCase(), Echiquier.ech.gcase(cases.getRange()-1,cases.getColonne()+1)))
    {
        tab[j]=Echiquier.ech.gcase(cases.getRange()-1, cases.getColonne()+1);
        j++;
    }
    if (Pion.verification(this.getCase(), Echiquier.ech.gcase(cases.getRange()+1,cases.getColonne()-1)))
    {
        tab[j]=Echiquier.ech.gcase(cases.getRange()-1, cases.getColonne()-1);
        j++;
    }
}

for(int w=3;j<w;w--)
{
    tab[j]=null;
}
]]></ATT>
